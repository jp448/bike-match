<h1>Bookings</h1>

<ul>
  <% @bookings.each do |booking|%>
    <li><%= link_to booking.id, booking_path(booking) %></li>
  <% end %>
</ul>

<%= link_to "Delete #{@booking.id}",
            booking_path(@booking),
            method: :delete,
            data: { confirm: "You want to delete the booking?" } %>